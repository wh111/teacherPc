.task_goon {
  .el-table td, .el-table th.is-leaf{
    background: #fff;
    //border:1px solid #e6ebf5;
  }
  .el-table thead tr{
    color: #a0a0a0;
  }
  .lookTips {
    cursor: pointer;
  }
  .task-content {
    margin: 10px 30px 20px 30px;
  }
  .title {
    padding: 10px 0;
    .text {
      margin-left: 10px;
      font-size: 24px;
      color: #434343;
    }
    .el-button {
      margin-top: 5px;
    }
    border-bottom: 1px solid #e5e5e5;
    font-size: 16px;
    line-height: 40px;
  }
  .tip {
    line-height: 50px;
    font-size: 13px;

    span {
      margin-right: 16px;
      width: 80px;
      display: inline-block;
      text-align: right;
      color: #a0a0a0;
    }
    .tips {
      display: inline-block;
      width: 760px;
      font-weight: 400;
      line-height: 21px;
    }
    border-bottom: 1px solid #f5f5f5;
  }
  .test {
    height: 20px;
    background: #efefef;
  }
  .bzst {
    // height:80px;
    background: #fff;
    margin: 0 30px 30px 30px;
  }
  .item {
    margin-top: 30px;
    border: 1px solid #e5e5e5;
    .task-item-title {
      padding: 0 7px;
      line-height: 39px;
      background: #efefef;
      font-size: 12px;
      .source {
        color: #716d6d;
      }
      .time {
        color: #a0a0a0;
      }
      .questionIndex {
        color: #11a73c;
        font-weight: bold;
      }
      span {
        margin: 5px
      }
    }
    .task-item-content {
      color: #434343;
      font-size: 12px;
      position: relative;
      .tip-wrap {
        display: inline-block;
        border-right: 1px solid #e5e5e5;
        font-size: 14px;
        width: 800px;
        .name {
          line-height: 30px;
          margin-left: 20px
        }
        .types {
          margin-top: 10px;
        }
      }
      .btn-wrap {
        text-align: center;
        width: 88px;
        position: absolute;
        right: 1px;
        top: 50%;
        transform: translateY(-50%);
        p {
          text-align: center;
        }
      }

    }
  }
}
