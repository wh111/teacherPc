
.selfData-root {
    width: 950px;
    min-height: 720px;
    padding: 40px 30px;
    border-radius: 5px;
    background-color: #ffffff;
    text-align: center;
    margin: 0 auto;
    .el-form-item--small.el-form-item {
        padding-bottom: 9px;
        border-bottom: 1px solid #eee;
        margin-top: 8px;
    }
    .selfDataSelectplace .el-form-item {
        width: 253px;
        float: left;
    }
    .selfDataSelectplace .el-select--small {
        width: 160px;
    }
    .el-form-item.is-success .el-input__inner{
        border-color: #b4bccc;
    }
    .el-tabs__nav-scroll {
        padding-left: 20px;
    }
    .selfDataSelectplace .el-form-item {
        width: 253px;
        float: left;
    }
    .selfDataSelectplace .el-select--small {
        width: 160px;
    }
    .el-form-item.is-success .el-input__inner {
        border-color: #b4bccc;
    }
    .el-input__inner:hover {
        border-color: #11A63F !important;
    }
    .el-radio__input.is-checked .el-radio__inner {
        border-color: #fd7416;
        background: #fd7416;
    }
    .el-radio__input.is-checked + .el-radio__label {
        color: #727272;
    }
    .el-radio__input.is-checked .el-radio__inner:hover {
        border-color: #11A63F;
        background: #11A63F;
    }
    .el-radio__input.is-checked + .el-radio__label:hover {
        color: #11A63F;
    }
    .el-radio__label:hover {
        color: #11A63F;
    }

    .avatar-uploader .el-upload {
        border: solid 6px #e5e5e5;
        border-radius: 100%;
        cursor: pointer;
        position: relative;
        overflow: hidden;
    }
    .avatar-uploader-icon {
        font-size: 28px;
        color: #8c939d;
        width: 100px;
        height: 100px;
        line-height: 100px;
        text-align: center;
    }
    .base-img {
        box-sizing: content-box;
        width: 100px;
        height: 100px;
        border: solid 6px #e5e5e5;
        margin-top: 34px;
        border-radius: 100%;
        overflow: hidden;
        margin-left: 390px;
        float: left;
        img {
            width: 100%;
            height: 100%;
        }
    }
    .base-change {
        margin-top: 30px;
        position: relative;
        .showdow {
            position: absolute;
            left: 0;
            top: 0;
            color: #ffffff;
            text-align: center;
            line-height: 100px;
            height: 100%;
            width: 100%;
            background: #313131;
            opacity: 0.5;
            visibility: hidden;
        }

    }
    .base-change:hover .showdow {
        visibility: visible;
    }
    .base-introduce {
        margin-top: 30px;
        text-align: left;
    }
    .selectBook {
        min-width: 100px;
        max-width: 260px;
        height: 30px;
        text-align: center;
        float: left;
        border: 1px solid #ccc;
        background: url("../../assets/images/selfdata (6).gif") no-repeat center center;
        border-radius: 4px;
        cursor: pointer;
        .adtive {
            width: 100px;
            height: 30px;
        }
        p {
            line-height: 30px;
        }
        .activeBook {
            border: 1px solid #1cc03b;
            display: block;
            min-width: 101px;
            height: 30px;
            border-radius: 4px;
            margin-top: -1px;
            margin-left: -1px;
            max-width: 261px;
            overflow: hidden;
            text-overflow: ellipsis;
            white-space: nowrap;
        }
    }
    .selectBookword {
        margin-right: 10px;
        height: 60px;
        float: left;
        text-align: center;
    }
    .submit-deserve {
        width: 110px;
        height: 40px;
        border-radius: 3px;
        background-color: #11a63f;
        border: none;
        font-family: MicrosoftYaHei;
        font-size: 13px;
        line-height: 40px;
        letter-spacing: normal;
        color: #ffffff;
    }
    .safeAccount {
        text-align: left;
        font-size: 14px;
        color: #5a5e66;
        line-height: 40px;
        .na {
            float: left;
            width: 80px;
            text-align: right;

        }
        span {
            padding-left: 12px;
            width: 120px;
            display: inline-block;
        }
        button {
            width: 60px;
            height: 30px;
            border-radius: 3px;
            background-color: #ffffff;
            border: solid 1px #cccccc;
            font-size: 12px;
            line-height: 30px;
            color: #898989;
            margin-right: 10px;
        }
        .relative {
            background-color: #fd7416;
            color: #fff;
            border: solid 1px #fd7416;
        }
        .p {
            border-top: 1px solid #eee;
            margin: 8px 0;
            padding-top: 10px;
            height: 50px;

        }
        .pBottom {
            border-bottom: 1px solid #eee;
            padding-bottom: 10px;
            height: 60px;
        }
    }
    .accessWord {
        cursor: pointer;
        width: 80px;
        height: 30px;
        border-radius: 3px;
        background-color: #fd7416;
        line-height: 30px;
        font-size: 12px;
        color: #ffffff;
        border: none;
        display: inline-block;
        text-align: center;
        margin-left: 10px;
    }
    .wechatLogin {
        line-height: 60px;
        font-size: 20px;

    }
}

.selectBook-con .ivu-modal-header {
    display: none;
}

.selectBook-con .ivu-modal-footer {
    display: none;
}

